# Supabase (онлайн, не локально)
# Отримайте з: https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Database (PostgreSQL для Payload CMS)
# Можна використовувати Supabase PostgreSQL з Settings → Database → Connection string (URI)
DATABASE_URL=postgresql://user:password@host:5432/database

# Payload CMS
# Генеруйте безпечний ключ: openssl rand -base64 32
PAYLOAD_SECRET=your-secret-key-min-32-chars-change-in-production
